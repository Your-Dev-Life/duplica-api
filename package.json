{
  "name": "duplica-api",
  "version": "0.0.1",
  "description": "Duplica API",
  "author": "Your Dev Life Team",
  "engines": {
    "node": "^14.0.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "start": "serverless offline --stage local",
    "watch": "tsc -w",
    "compile": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "test": "jest --runInBand --detectOpenHandles --colors --verbose",
    "test:ci": "npm run lint && npm run test:coverage",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --runInBand --no-cache --coverage --silent",
    "dbgui": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin",
    "db:install": "serverless dynamodb install",
    "db:start": "serverless dynamodb start --stage local",
    "db:migrate": "serverless dynamodb migrate",
    "db:syncDevToLocal": "cd dev && sh syncDynamoDBFromDevToLocal.sh",
    "deploy": "npm run compile && serverless deploy --verbose --stage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.36.0",
    "@aws-sdk/util-dynamodb": "^3.36.0",
    "@dinero.js/currencies": "^2.0.0-alpha.8",
    "dayjs": "^1.10.6",
    "dinero.js": "^2.0.0-alpha.8",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "merge": "^2.1.1",
    "nanoid": "^3.1.23",
    "node-fetch": "^3.0.0",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@shelf/jest-dynamodb": "^2.1.0",
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^27.0.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.10.3",
    "@types/node-fetch": "^3.0.3",
    "@types/prettier": "^2.3.2",
    "@types/ramda": "^0.27.44",
    "@types/serverless": "^1.78.33",
    "@types/serverless-jest-plugin": "^0.3.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "dotenv": "^10.0.0",
    "dynamodb-admin": "^4.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "mockdate": "^3.0.5",
    "prettier": "^2.3.2",
    "serverless": "^2.53.1",
    "serverless-associate-waf": "^1.2.1",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-functions-base-path": "^1.0.32",
    "serverless-jest-plugin": "^0.4.0",
    "serverless-newrelic-lambda-layers": "^2.1.4",
    "serverless-offline": "^8.0.0",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:coverage && npm run lint"
    }
  }
}
